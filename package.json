{
  "name": "@aiviagroup/keyvault-sync",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm nx run-many -t generate && husky install",
    "setup-ci": "./libs/workspace/src/scripts/setup-ci.sh",
    "format-check": "pnpm nx format:check --all --uncommitted",
    "format-write": "pnpm nx format:write --all",
    "az-login": "az account show || az login",
    "gen-env-files": "./tools/scripts/populate-kv-secret-env-files.sh -k aiviagroup-keyvault-sync-repo-local",
    "smoke-test": "pnpm nx format:check --all && pnpm nx run-many -t lint && pnpm nx run-many -t build && pnpm nx run-many -t test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm nx format:write --all"
    }
  },
  "private": true,
  "dependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
